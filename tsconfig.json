{
    "include": [
        "./src/**/*.*"
    ],
    "compilerOptions": {
        "lib": [
            "DOM",
            "DOM.Iterable",
            "ESNext"
        ],
        "moduleResolution": "Node",
        "target": "ESNext",
        "jsx": "react",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true
    }
}